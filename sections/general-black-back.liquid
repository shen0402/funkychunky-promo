                    <a href="{{ product.url }}" class="boost-pfs-filter-product-item-title">{{ product.title }}</a>
                    <span class="break-line"></span>
                  </h2>

                  {% comment %}
                  {% if product.metafields.product_info.collection_description %}
                  <p class="black-text smaller">{{ product.metafields.product_info.collection_description }}</p>
                  {% endif %}
                  {% endcomment %}

                </div>

                <p class="product-price-collection">{{ product.price | money }}</p>

                <a href="{{product.url}}" class="general-btn dark-with-w-text margin-t w-button">Shop Now</a>

              </div>

            {% endunless %}



          {% endfor %}
        </div>
