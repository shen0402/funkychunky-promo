          {% for product in collections["best-selling-gifts"].products %}


            {%- assign iteration_number = forloop.index -%}
            {% assign current_variant = product.selected_or_first_available_variant %}

            {% unless iteration_number > 10 %}

              <div class="shop-flex-col-item center home">
